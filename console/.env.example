# Console Admin Credentials
CONSOLE_ADMIN_USER=admin
CONSOLE_ADMIN_PASSWORD=change-this-secure-password

# RCON Configuration (must match server.properties)
RCON_HOST=host.docker.internal
RCON_PORT=25575
RCON_PASSWORD=change-this-rcon-password

# Session Configuration
SESSION_SECRET=generate-a-random-32-char-string-here

# Minecraft Server Path (inside container)
MC_SERVER_DIR=/minecraft

# Environment
NODE_ENV=production
